
<!-- =========================
    HOME SECTION   
============================== -->
<section id="home" class="parallax-section"  {{attributes}} background="{{bg_imgs}}">
     <div class="container">
          <div class="row">
               <div class="col-md-offset-1 col-md-10 col-sm-12">
                    <h3 class="wow bounceIn" data-wow-delay="0.9s">{{content.field_subtitle.0}}</h3>
                    <h1 class="wow fadeInUp" data-wow-delay="1.6s">{{content.field_ftitle.0}}</h1>
                    <a href="{{content.field_link.0['#url']}}" class="wow fadeInUp smoothScroll btn btn-default" data-wow-delay="2s">
                      {{content.field_link.0['#title']}}
                    </a>
               </div>
          </div>
     </div>
</section>