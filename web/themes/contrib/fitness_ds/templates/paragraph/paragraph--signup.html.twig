
<!-- =========================
    NEWSLETTER SECTION   
============================== -->
<section id="newsletter" class="parallax-section"  style="background: url({{ file_url(content.field_background_image['#items'].entity.uri.value)}}) no-repeat center top;" {{attributes}}>
	<div class="container">
		<div class="row">

			<div class="wow fadeInUp col-md-offset-2 col-md-8 col-sm-offset-1 col-sm-10" data-wow-delay="0.9s">
				<h2>{{content.field_title.0}}</h2>
				<p>{{content.field_description.0}}</p>
				<div class="newsletter_detail">
					{{content.field_signup_form}}
				</div>
			</div>

		</div>
	</div>
</section>
